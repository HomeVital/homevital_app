{
	"cli": {
		"version": ">= 14.7.1",
		"appVersionSource": "remote",
		"requireCommit": true
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"environment": "development"
		},
		"ios-simulator": {
			"extends": "development",
			"ios": {
				"simulator": true
			}
		},
		"preview": {
			"distribution": "internal",
			"environment": "preview"
		},
		"production": {
			"autoIncrement": true,
			"environment": "production"
		}
	},
	"submit": {
		"production": {}
	}
}
